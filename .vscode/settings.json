{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.quickSuggestions": {
    "other": "on",
    "comments": "off",
    "strings": "on"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "driver": "SQLite",
      "name": "grad-data",
      "database": "${workspaceFolder:grad-data}/sqlite/grad-data.db"
    }
  ],
  "sqltools.useNodeRuntime": true
}